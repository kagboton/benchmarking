;$(document).ready(function(){var n=$('div#vehicule_composantsAffichage'),e=n.find(':input').length;$('#add_composantAffichage').click(function(e){t(n);e.preventDefault();return!1});if(e==0){t(n)}
else{n.children('div').each(function(){a($(this))})};function t(n){var c=n.attr('data-prototype').replace(/__name__label__/g,'Composant d\'affichage nÂ°'+(e+1)).replace(/__name__/g,e),t=$(c);a(t);n.append(t);e++};function a(n){var e=$('<a href="#" class="btn btn-danger"><span class="glyphicon glyphicon-trash"></span> Supprimer</a>');n.append(e);e.click(function(e){n.remove();e.preventDefault();return!1})}});