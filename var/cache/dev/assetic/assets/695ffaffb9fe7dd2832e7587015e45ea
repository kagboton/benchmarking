;$(document).ready(function(){var e=$('div#vehicule_composantsAffichage'),n=e.find(':input').length;$('#add_composantAffichage').click(function(n){t(e);n.preventDefault();return!1});if(n==0){t(e)}
else{e.children('div').each(function(){a($(this))})};function t(e){var c=e.attr('data-prototype').replace(/__name__label__/g,'Composant d\'affichage nÂ°'+(n+1)).replace(/__name__/g,n),t=$(c);a(t);e.append(t);n++};function a(e){var n=$('<a href="#" class="btn btn-danger"><span class="glyphicon glyphicon-remove"></span>Supprimer</a>');e.append(n);n.click(function(n){e.remove();n.preventDefault();return!1})}});